import java.util.*;
  
public class Main {
  public static void main(String[] args){
    Scanner in=new Scanner(System.in);
    int n = in.nextInt();
    String[] tmp = new String[n];
    for(int i=0; i<n; i++){
      tmp[i] = in.next();
    }
    
    List<String> answer = new ArrayList<>();
    
    for(String s : tmp){
      String s2 = new StringBuilder(s).reverse().toString();
      answer.add(s2);
    }
    
	for(String a : answer){
      System.out.println(a);
    }
  }
}
